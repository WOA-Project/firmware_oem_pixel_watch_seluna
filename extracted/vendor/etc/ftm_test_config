tc 1
#Left Speaker
!Playback
gkv_rx:PCM_LL_PLAYBACK-SPEAKER-INSTANCE1-AUDIO_MBDRC
be_name_rx:MI2S-LPAIF_RXTX-RX-PRIMARY
pcm_id_rx:100
channels_rx:1

enable
CDC_RX0_MUX:I2S0_D2_L
RX0_SWITCH Switch:1
SPKR_OUT_S Switch:1

disable
CDC_RX0_MUX:ZERO
RX0_SWITCH Switch:0
SPKR_OUT_S Switch:0

tc 2
#SWR DMIC1
!Capture
gkv_tx:PCM_RECORD-SPEAKER_MIC-AUDIO_FLUENCE_SMECNS-INSTANCE1
be_name_tx:MI2S-LPAIF_VA-TX-PRIMARY
pcm_id_tx:101
channels_tx:1
Txdevice:0

enable
TX0_MIXER TX0_CAP:1
I2S0_D0_L_MUX:CDC_TX1
TXCH1 Switch:1
TX1_INP_MUX:DMIC_1
DMIC1_MUX:DMIC1
DMIC1_IN_S Switch:1
BB_MIC_BIAS1:BIAS_ON

disable
TX0_MIXER TX0_CAP:0
I2S0_D0_L_MUX:ZERO
TXCH1 Switch:0
TX1_INP_MUX:ZERO
DMIC1_MUX:ZERO
DMIC1_IN_S Switch:0
BB_MIC_BIAS1:ZERO

