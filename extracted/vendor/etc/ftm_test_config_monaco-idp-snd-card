tc 1
#Left Speaker
!Playback
gkv_rx:PCM_LL_PLAYBACK-SPEAKER-INSTANCE1-AUDIO_MBDRC
be_name_rx:CODEC_DMA-LPAIF_RXTX-RX-0
pcm_id_rx:100
channels_rx:1

enable
RX_MACRO RX0 MUX:AIF1_PB
RX INT2_1 MIX1 INP0:RX0
SWR DAC_Port Switch:1
RX_RX2 Digital Volume:84

disable
RX_MACRO RX0 MUX:ZERO
RX INT2_1 MIX1 INP0:ZERO
SWR DAC_Port Switch:0

tc 2
#SWR DMIC2
!Capture
gkv_tx:PCM_RECORD-SPEAKER_MIC-AUDIO_FLUENCE_SMECNS-INSTANCE1
be_name_tx:CODEC_DMA-LPAIF_RXTX-TX-3
pcm_id_tx:101
channels_tx:1
Txdevice:0

enable
TX DEC2 MUX:MSM_DMIC
TX_AIF1_CAP Mixer DEC2:1
TX DMIC MUX2:DMIC2
TX_DEC2 Volume:84

disable
TX DEC2 MUX:ZERO
TX_AIF1_CAP Mixer DEC2:0
TX DMIC MUX2:ZERO

